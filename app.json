{
  "scheme": ["resbacapp"],
  "name": "RESBAC1",
  "slug": "RESBAC1",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "userInterfaceStyle": "light",
  "newArchEnabled": true,
  "splash": {
    "image": "./assets/splash-icon.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  },
  "ios": {
    "supportsTablet": true,
    "infoPlist": {
      "LSApplicationQueriesSchemes": [
        "tel",
        "fb-messenger"
      ]
    }
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "edgeToEdgeEnabled": true,
    "package": "com.lukewarmraven.RESBAC1",
    "intentFilters": [
      {
        "action": "VIEW",
        "data": [
          {
            "scheme": "tel"
          },
          {
            "scheme": "https",
            "host": "m.me"
          }
        ],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ]
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "plugins": [
    "expo-router",
    "expo-font"
  ],
  "extra": {
    "router": {},
    "eas": {
      "projectId": "c9f09e1b-da31-40a2-982b-74d060f39cc9"
    }
  },
  "sdkVersion": "54.0.0",
  "platforms": [
    "ios",
    "android",
    "web"
  ]
}