{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android":{
        "buildType":"apk",
        "extraFiles": [
          {
            "src": "./google-services.json",
            "dest": "app/google-services.json"
          }
        ]
      }
    },
    "preview": {
      "distribution": "internal",
      "android":{
        "buildType":"apk"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
