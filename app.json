{
  "scheme": ["resbacapp"],
  "name": "RESBAC",
  "slug": "RESBAC1",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "userInterfaceStyle": "light",
  "newArchEnabled": true,
  "splash": {
    "image": "./assets/splash-icon.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/icon.png",
      "backgroundColor": "#ffffff"
    },
    "edgeToEdgeEnabled": true,
    "package": "com.lukewarmraven.resbac",
    "googleServicesFile": "./google-services.json",
    "intentFilters": [
      {
        "action": "VIEW",
        "data": [
          {
            "scheme": "tel"
          },
          {
            "scheme": "https",
            "host": "m.me"
          }
        ],
        "category": ["BROWSABLE", "DEFAULT"]
      }
    ]
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "plugins": [
    "expo-router",
    "expo-font",
    "expo-sqlite",
    [
      "expo-notifications",
      {
        "icon": "./assets/notif-icon.png",
        "color": "#0060ff",
        "androidMode": "default",
        "androidCollapsedTitle": "RESBAC Alert"
      }
    ]
  ],
  "extra": {
    "router": {},
    "eas": {
      "projectId": "c9f09e1b-da31-40a2-982b-74d060f39cc9"
    }
  },
  "sdkVersion": "54.0.0",
  "platforms": ["ios", "android", "web"],
  "owner": "lukewarmraven"
}
